generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Jemaat {
  id String @id @default(cuid())
  ibadah String
  nama String
  whatsapp String
  usia String
  alamat String
  komsel String
  KK String
  GKK String
  isVerified Boolean @default(false)
  link String 
}

model Quota {
  id Int @id @default(autoincrement())
  KU1 Int
  KU2 Int
  KU3 Int
}